{
  "name": "tlapnet/modalus",
  "description": "Modals",
  "keywords": [
    "modals",
    "nette",
    "bootstrap"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/tlapnet/modalus",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "contributte/utils": "^0.4.0",
    "doctrine/annotations": "^1.6",
    "latte/latte": "^2.5.1",
    "nette/application": "~3.0.0",
    "nette/di": "~3.0.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.10.0",
    "phpstan/extension-installer": "^1.0.0",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-shim": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Modalus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Tlapnet\\Modalus\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
